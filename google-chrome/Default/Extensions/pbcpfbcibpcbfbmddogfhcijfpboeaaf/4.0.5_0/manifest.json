{
   "action": {
      "default_icon": "assets/images/icon-disabled.png",
      "default_popup": "index.html"
   },
   "background": {
      "service_worker": "worker.js",
      "type": "module"
   },
   "commands": {
      "_execute_browser_action": {
         "suggested_key": {
            "default": "Ctrl+Shift+Space",
            "mac": "MacCtrl+Shift+Space"
         }
      },
      "devtools-panel": {
         "description": "Open Amino DevTools",
         "suggested_key": {
            "default": "Alt+Shift+Space"
         }
      }
   },
   "content_scripts": [ {
      "js": [ "assets/js/lib/sass/sass.sync.min.js", "chrome/inject/apply-styles.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_start"
   }, {
      "css": [ "assets/css/web-components.css" ],
      "js": [ "chrome/inject/mount-web-components.js", "chrome/inject/extension-message-handler.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_end"
   } ],
   "default_locale": "en",
   "description": "__MSG_appDesc__",
   "devtools_page": "chrome/devtools/loader.html",
   "externally_connectable": {
      "matches": [ "*://*.app.aminoeditor.com/*", "*://app.aminoeditor.com/*", "*://*.aminoeditor.com/*" ]
   },
   "host_permissions": [ "https://*/*", "http://*/*" ],
   "icons": {
      "128": "assets/images/icon-128.png",
      "16": "assets/images/icon-16.png",
      "24": "assets/images/icon-24.png",
      "32": "assets/images/icon-32.png",
      "48": "assets/images/icon-48.png",
      "512": "assets/images/icon-512.png",
      "64": "assets/images/icon-64.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "__MSG_appName__",
   "permissions": [ "activeTab", "storage", "contextMenus", "tabs", "scripting", "unlimitedStorage" ],
   "short_name": "Amino",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "4.0.5",
   "web_accessible_resources": [ {
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "amino-inspect.min.js", "assets/css/lib/prism.css", "assets/images/icon-context.png", "assets/js/lib/webcomponents-bundle.js" ]
   } ]
}
